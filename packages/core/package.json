{
  "name": "sungrow-ui",
  "version": "1.0.0",
  "description": "Components libary for Vue 3 + Ts",
  "files": [
    "dist"
  ],
  "main": "./dist/umd/index.umd.cjs",
  "type": "module",
  "module": "./dist/es/index.js",
  "types": "./dist/types/core/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/core/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/umd/index.umd.cjs"
    },
    "./dist/": {
      "import": "./dist/",
      "require": "./dist/"
    }
  },
  "sideEffects": [
    "./dist/index.css"
  ],
  "scripts": {
    "build": "run-p build-umd build-es",
    "build:watch": "run-p build-umd:watch build-es:watch",
    "build-umd": "vite build --config vite.umd.config.ts",
    "build-es": "vite build --config vite.es.config.ts",
    "build-umd:watch": "vite build --config vite.umd.config.ts --watch",
    "build-es:watch": "vite build --config vite.es.config.ts --watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "release": "release-it"
  },
  "peerDependencies": {
    "vue": "^3.4.27"
  },
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.31.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-compression2": "^1.1.3",
    "vite-plugin-dts": "3.9.1"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.6",
    "@popperjs/core": "^2.11.8",
    "async-validator": "^4.2.5"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
